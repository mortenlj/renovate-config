{
	"extends": [
		":automergeAll",
		":automergeBranch",
		":automergeDigest",
		":automergeLinters",
		":automergeTesters",
		":automergeRequireAllStatusChecks",
		"config:best-practices"
	],
	"ignorePresets": [
		":dependencyDashboard"
	],
	"prConcurrentLimit": 5,
	"prHourlyLimit": 2,
	"packageRules": [
		{
			"groupName": "Rust OpenTelemetry packages",
			"groupSlug": "opentelemetry-rust",
			"matchDatasources": [
				"crate"
			],
			"matchPackageNames": [
				"*opentelemetry*"
			]
		}
		{
			"groupName": "Rust Kubernetes packages",
			"groupSlug": "kubernetes-rust",
			"matchDatasources": [
				"crate"
			],
			"matchPackageNames": [
				"kube",
				"k8s-openapi",
				"json-patch",
				"jsonptr"
			]
		},
		{
			"groupName": "Github actions",
			"groupSlug": "github-actions",
			"matchDatasources": [
				"github-tags"
			],
			"matchManagers": [
				"github-actions"
			]
		}
	]
}
